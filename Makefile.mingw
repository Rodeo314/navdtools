CC          = x86_64-w64-mingw32-gcc
CFLAGS      = -O3 -std=gnu99 -DMINGW_HAS_SECURE_API -DCOMPAT_STRCASESTR=1 -DCOMPAT_STRERROR_R=1 -DCOMPAT_STRNDUP=1 -DCOMPAT_STRSEP=1
NDCONV_EXE  = navdconv.exe
TARGET_ARCH =

all:
	$(MAKE) -f Makefile CC="$(CC)" CFLAGS="$(CFLAGS)" TARGET_ARCH="$(TARGET_ARCH)" NDCONV_EXE="$(NDCONV_EXE)" all

.PHONY: clean
clean:
	$(MAKE) -f Makefile NDCONV_EXE="$(NDCONV_EXE)" clean
